{
  "name": "untested",
  "version": "0.1.5",
  "description": "Orders tests for a source code change based on code coverage analysis.",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/untested.git"
  },
  "scripts": {
    "test": "grunt && node untested.js --info"
  },
  "pre-commit": ["npm test", "npm version"],
  "pre-push": "rm -rf node_modules && npm install && npm test",
  "keywords": [
    "js",
    "javascript",
    "testing",
    "quality",
    "changelist",
    "commit"
  ],
  "dependencies": {
    "check-types": "2.2.0",
    "ggit": "0.6.2",
    "gt": "0.8.47",
    "lasso-node": "0.1.13",
    "lodash": "~4.17.19",
    "optimist": "0.6.1"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "license": "MIT",
  "preferGlobal": "true",
  "bin": {
    "untested": "./untested.js"
  },
  "main": "./untested.js",
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-bump": "0.0.16",
    "grunt-contrib-jshint": "0.10.0",
    "pre-git": "0.1.1"
  }
}
